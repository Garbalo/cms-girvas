<IfModule mod_rewrite.c>
	RewriteEngine On
	
	Options -Indexes
	
	RewriteBase /

	RewriteCond %{HTTP_HOST} ^cms\-girvas\.ru [NC]
	RewriteRule ^(.*)$ https://www.cms-girvas.ru/$1 [NC,R=301,L]
	
	RewriteCond %{REQUEST_FILENAME} !-f
	RewriteCond %{REQUEST_FILENAME} !-d
	RewriteRule ^(.*)$ ?$1 [QSA,L]
</IfModule>